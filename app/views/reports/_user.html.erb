<tr id="user_<%= user.id %>">
    <td><%= user.username %></td>
    <td><%= user.email %></td>
    <td>
	    <%= link_to 'Edit', edit_user_path(user), remote: true, :class => 'btn btn-sm btn-primary' %>
	</td>
    <td>
	    <%= link_to 'Delete', user, remote: true, :method => :delete, data: {confirm: "Delete User: #{user.id}"}, :class => 'btn btn-sm btn-primary' %>
	</td>
</tr>